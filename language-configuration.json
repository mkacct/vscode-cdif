{
	"comments": {
		"lineComment": "//",
		"blockComment": ["/*", "*/"]
	},
	"brackets": [
		["{", "}"],
		["[", "]"]
	],
	"autoClosingPairs": [
		{"open": "{", "close": "}", "notIn": ["string"]},
		{"open": "[", "close": "]", "notIn": ["string"]},
		{"open": "(", "close": ")", "notIn": ["string"]},
		{"open": "'", "close": "'", "notIn": ["string"]},
		{"open": "\"", "close": "\"", "notIn": ["comment", "string"]},
		{"open": "`", "close": "`", "notIn": ["comment", "string"]}
	]
}
